{
	"name": "mreschke/mrcore",
	"description": "A Wiki/CMS built with Laravel 5",
	"keywords": ["mrcore", "wiki", "cms", "framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=5.5.9",
		"laravel/framework": "5.1.*",
		"illuminate/html": "~5.0",
		"guzzlehttp/guzzle": "~6.0",
		"predis/predis": "~1.0",
		"sabre/dav": "~1.8.7",
		"camspiers/porter-stemmer": "1.0.0",
		"easybook/geshi": "~1.0.8.12"
	},
	"require-dev": {
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "0.9.*",
		"phpspec/phpspec": "~2.1",
		"phpunit/phpunit": "~4.0",
		"filp/whoops": "~1.1.3",
		"barryvdh/laravel-debugbar": "~2.0"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"Mrcore\\": "app/",
			"Mrcore\\Modules\\": "../Modules/",
			"Mrcore\\Themes\\": "../Themes/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
